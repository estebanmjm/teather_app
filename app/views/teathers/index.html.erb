<p id="notice"><%= notice %></p>

<h1>Listing Teathers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Manager</th>
      <th>Mobile</th>
      <th>Phone</th>
      <th>Web site</th>
      <th>Logo</th>
      <th>Description</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teathers.each do |teather| %>
      <tr>
        <td><%= teather.name %></td>
        <td><%= teather.address %></td>
        <td><%= teather.manager %></td>
        <td><%= teather.mobile %></td>
        <td><%= teather.phone %></td>
        <td><%= teather.web_site %></td>
        <td><%= teather.logo %></td>
        <td><%= teather.description %></td>
        <td><%= teather.latitude %></td>
        <td><%= teather.longitude %></td>
        <td><%= link_to 'Show', teather %></td>
        <td><%= link_to 'Edit', edit_teather_path(teather) %></td>
        <td><%= link_to 'Destroy', teather, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teather', new_teather_path %>
